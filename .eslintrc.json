{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "webextensions": true
  },
  "extends": ["eslint:recommended", "prettier"],
  "globals": {
    "_": "readonly",
    "HNSpecial": "readonly"
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": ["warn", { "vars": "all", "args": "all" }],
    "no-undef": ["warn"],
    "no-proto": ["error"],
    "prefer-spread": ["warn"]
  }
}
